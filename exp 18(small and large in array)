#include<stdio.h>
int main()
{
	int arr[100],n,small,large,i;
	printf("enter the number of elements that you want to insert:");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		printf("enter element%d:",i+1);
		scanf("%d",&arr[i]);
	}
	small= arr[0];
	large=arr[0];
	for(i=1;i<n;i++)
	{
		if(arr[i]<small)
		{
			small=arr[i];
		}
		if(arr[i]>large)
		{
			large=arr[i];
		}
	}
	printf("\nlargest element is:%d",large);
	printf("\n smallest element is:%d",small);
	returnÂ 0;
}

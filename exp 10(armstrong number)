#include<stdio.h>
int main()
{
	int n,r,sum=0,temp;
	printf("\n enter n value:");
	scanf("%d",&n);
	temp=n;
	printf("\n given number=%d",n);
	while(n>0)
	{
		r=n%10;
		sum=sum+(r*r*r);
		n=n/10;
	}
	if(temp==sum)
	   printf(" is a armstrong number");
	   else
	   printf( " is not a armstrong number");
	   return 0;	
}
